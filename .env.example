# Configuração do Banco de Dados (MySQL Remoto)
DB_USER=seu_usuario_mysql
DB_PASS=sua_senha_mysql
DB_NAME=nome_do_banco
DB_HOST=host_do_banco
DB_PORT=3306
DB_DIALECT=mysql
DB_TIMEZONE=-03:00

# Configuração do Servidor
PORT=3000
NODE_ENV=development

# Segurança
# Gere um session secret usando: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
SESSION_SECRET=sua_chave_secreta_aqui
